{
	"name": "codekandis/json-error-handler",
	"description": "`codekandis/json-error-handler` is a library to handle JSON errors in an object oriented way.",
	"version": "3.0.0",
	"keywords": [
		"php",
		"json error handler",
		"json",
		"error",
		"handler"
	],
	"minimum-stability": "stable",
	"prefer-stable": true,
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Ramelow",
			"email": "info@codekandis.net",
			"homepage": "https://codekandis.net",
			"role": "Developer"
		}
	],
	"support": {
		"email": "info@codekandis.net",
		"issues": "https://github.com/codekandis/json-error-handler/issues",
		"docs": "https://github.com/codekandis/json-error-handler/blob/master/README.md",
		"source": "https://github.com/codekandis/json-error-handler/tree/master"
	},
	"require": {
		"php": ">=8.3",
		"ext-json": "*",
		"codekandis/constants-classes-translator": "^2.0.0",
		"codekandis/types": "^1.0.0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-master",
		"codekandis/phpunit": "^5.0.0",
		"rector/rector": "^1.0.5"
	},
	"autoload": {
		"psr-4": {
			"CodeKandis\\JsonErrorHandler\\": [
				"src/"
			]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"CodeKandis\\JsonErrorHandler\\Build\\": [
				"build/"
			],
			"CodeKandis\\JsonErrorHandler\\Tests\\": [
				"tests/"
			]
		}
	}
}
